<?xml version="1.0"?>
<!-- ==============================================================================
   Messaging Configuration
  ============================================================================== -->
<messaging-config
  defaultServer="#messaging"
  xmlns="http://guidewire.com/messaging-config">
  <messaging>
    <!-- Email Messaging destination
DO NOT CHANGE THE id OF THIS DESTINATION -->
    <destination
      id="65"
      initialretryinterval="500"
      internal="true"
      maxretries="5"
      name="Java.MessageDestination.Email.Name"
      transportplugin="emailMessageTransport"/>
    <destination
      id="100"
      internal="true"
      name="Test destination"
      pollinterval="500"
      replyplugin="testReply"
      requestplugin="testRequest"
      transportplugin="testTransport">
      <event
        name="(\w)*"/>
    </destination>
    <destination
      id="101"
      internal="true"
      name="messageTestDestination"
      replyplugin="messageTestReply"
      requestplugin="messageTestRequest"
      transportplugin="messageTestTransport">
      <event
        name="MessageItemAdded"/>
      <event
        name="NoteAdded"/>
      <event
        name="ActivityChanged"/>
      <event
        name="test Custom Event"/>
      <event
        name="MessageItemResync"/>
      <event
        name="EventAwareSubEntityAdded"/>
      <event
        name="EventAwareSubEntityChanged"/>
      <event
        name="EventAwareSubEntityRemoved"/>
      <event
        name="EventAwareCustom"/>
    </destination>
    <destination
      id="111"
      initialretryinterval="100"
      internal="true"
      name="Test workflow"
      transportplugin="consoleTransport">
      <event
        name="(\w)*"/>
    </destination>
    <destination
      id="201"
      internal="true"
      name="Test Contact Destination"
      primaryentity="Contact"
      transportplugin="contactTransport">
      <event
        name="ContactAdded"/>
      <event
        name="ContactChanged"/>
      <event
        name="ContactRemoved"/>
      <event
        name="MessageItemAdded"/>
      <event
        name="MessageItemChanged"/>
    </destination>
    <destination
      id="202"
      initialretryinterval="100"
      internal="true"
      name="Test Blocking Contact Destination"
      pollinterval="1000"
      primaryentity="Contact"
      singlethreadandblockonnonsafeorderedmessages="true"
      transportplugin="contactTransport">
      <event
        name="ContactAdded"/>
      <event
        name="ContactChanged"/>
      <event
        name="ContactRemoved"/>
      <event
        name="MessageItemAdded"/>
      <event
        name="MessageItemChanged"/>
    </destination>
    <destination
      id="324"
      internal="true"
      name="DocumentStore"
      transportplugin="documentStoreTransport">
      <event
        name="DocumentStore"/>
      <event
        name="FailedDocumentStore"/>
      <event
        name="DocumentStore=.*"/>
    </destination>
    <destination
      id="401"
      internal="true"
      name="Test TestGraph Destination"
      primaryentity="TestGraphRoot"
      transportplugin="testGraphTransport">
      <event
        name="TestGraph.*"/>
    </destination>
    <destination
      disabled="true"
      id="501"
      internal="true"
      name="Test hang on suspend"
      transportplugin="testHangOnSuspend">
      <event
        name="GroupAdded"/>
    </destination>
    <destination
      disabled="true"
      id="122"
      internal="true"
      name="Test Java Message Reply Plugin"
      replyplugin="InboundFileIntegrationMessageReply"
      transportplugin="InboundFileIntegrationMessageReplyTransport"/>
    <destination
      id="70"
      internal="true"
      name="Java.MessageDestination.SolrMessageTransport.Journal.Name"
      transportplugin="IPXJournalSolrMessageTransportPlugin">
      <event
        name="TestSolrJournalAdded"/>
      <event
        name="TestSolrJournalChanged"/>
      <event
        name="TestSolrJournalRemoved"/>
      <event
        name="TestSolrArticleAdded"/>
      <event
        name="TestSolrArticleChanged"/>
      <event
        name="TestSolrArticleRemoved"/>
    </destination>
    <destination
      id="71"
      internal="true"
      name="Java.MessageDestination.SolrMessageTransport.Dates.Name"
      transportplugin="IPXDatesSolrMessageTransportPlugin"/>
    <destination
      id="124"
      internal="true"
      name="Java.MessageDestination.Kafka"
      transportplugin="KafkaMessageTransport">
      <event
        name="\w*"/>
    </destination>
  </messaging>
</messaging-config>
